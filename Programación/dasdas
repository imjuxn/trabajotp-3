def clasificar_terremoto():
    magnitud = float(input("Por favor, ingrese la magnitud del terremoto: "))

    if magnitud < 3:
        print("Muy leve (imperceptible)")
    elif 3 <= magnitud < 4:
        print("Leve")
    elif 4 <= magnitud < 5:
        print("Moderado")
    elif 5 <= magnitud < 6:
        print("Fuerte")
    elif 6 <= magnitud < 7:
        print("Muy fuerte")
    elif 7 <= magnitud < 8:
        print("Extremadamente fuerte")
    elif magnitud >= 8:
        print("Catastrófico")
    else:
        print("Valor inválido")

clasificar_terremoto()